        require(
            mintVerifier.mintVerify(_a, _b, _c, _publicInputs),
            "Proof is not valid"
        );
        _mint(msg.sender, tokenId);
        storageproviders[tokenId].cHash = _publicInputs[0];
        storageproviders[tokenId].isRevealed = false;
        ogOwners[tokenId] = msg.sender;
        tokenId++;
    }

