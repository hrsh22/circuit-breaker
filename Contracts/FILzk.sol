        uint8 _tokenId
    ) public {
        require(revealVerifier.verifyRevealProof(_a, _b, _c, _publicInputs));
        require(
            storageproviders[_tokenId].cHash == _publicInputs[0],
            "Invalid hash"
        );
        storageproviders[_tokenId].attribute1 = _publicInputs[1];
        storageproviders[_tokenId].attribute2 = _publicInputs[2];
        storageproviders[_tokenId].attribute3 = _publicInputs[3];
        storageproviders[_tokenId].isRevealed = true;
    }

